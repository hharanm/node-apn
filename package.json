{
  "name": "@parse/node-apn",
  "description": "An interface to the Apple Push Notification service for Node.js",
  "version": "5.1.3",
  "author": "Parse Platform, Andrew Naylor <argon@mkbot.net>",
  "keywords": [
    "apple",
    "push",
    "push notifications",
    "iOS",
    "apns",
    "notifications"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/parse-community/node-apn.git"
  },
  "dependencies": {
    "debug": "4.3.3",
    "jsonwebtoken": "9.0.0",
    "node-forge": "1.3.1",
    "verror": "1.10.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "coverage": "node_modules/.bin/nyc --reporter=lcov node_modules/.bin/mocha 'test/**/*.js'",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier",
    "eslint": "eslint --cache ./",
    "eslint:fix": "eslint --cache --fix ./",
    "prettier": "prettier --write {lib,test,examples,mock}/{**/*,*}.js index.js",
    "test": "node_modules/.bin/mocha 'test/**/*.js'"
  },
  "jshintConfig": {
    "node": true
  },
  "eslintConfig": {
    "ecmaVersion": 6,
    "env": {
      "es6": true,
      "node": true
    }
  },
  "engines": {
    "node": ">= 12"
  },
  "license": "MIT"
}
